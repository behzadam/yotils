{
  "name": "yotils",
  "version": "1.0.1",
  "description": "Typescript utils",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "browser": "./dist/index.umd.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "generate": "yarn api-extractor run --local",
    "docs": "yarn api-documenter markdown -i temp -o docs",
    "clean": "rimraf dist lib",
    "compile": "tsc",
    "prebuild": "yarn run generate && yarn run docs",
    "build": "yarn run clean && rollup --config rollup.config.ts --configPlugin typescript",
    "release:npm": "npm publish dist"
  },
  "repository": {
    "type": "git",
    "url": "github:behzadam/yotils"
  },
  "homepage": "https://github.com/behzadam/yotils",
  "keywords": [
    "typescript",
    "utils"
  ],
  "devDependencies": {
    "@microsoft/api-documenter": "^7.19.24",
    "@microsoft/api-extractor": "^7.33.6",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^10.0.1",
    "@tsconfig/node12": "^1.0.11",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.28.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "rollup": "^3.7.3",
    "rollup-plugin-api-extractor": "^0.2.5",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@swc/core": "^1.3.22",
    "rollup-plugin-swc3": "^0.8.0"
  }
}
